{
  "name": "easybank-landing-page",
  "version": "1.0.0",
  "description": "Easybank Landing Page (Frontendmentor.io)",
  "main": "app.js",
  "dependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "node-sass": "^4.14.1",
    "sass": "^1.26.10"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch_sass": "node-sass --watch ./scss -o css",
    "serve": "node app.js",
    "sass": "node-sass ./scss -o css",
    "sass:watch": "npm run sass -- --watch",
    "dev_start": "concurrently --names \"SASS,WATCH,SERVE\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run sass\" \"npm run sass:watch\" \"npm run serve\"",
	"startDev": "npm run dev_start"
  },
  "keywords": [
    "easybank",
    "frontendmentor",
    "frontendmentor.io"
  ],
